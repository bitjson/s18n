{
  "name": "s18n",
  "version": "2.1.0",
  "description": "Semantic localization for html.",
  "main": "lib/s18n.js",
  "bin": {
    "s18n": "bin/s18n"
  },
  "scripts": {
    "test": "gulp test",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitjson/s18n"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "localize",
    "l10n",
    "translate",
    "semantic"
  ],
  "author": "Jason Dreyzehner <jason@dreyzehner.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bitjson/s18n/issues"
  },
  "homepage": "https://github.com/bitjson/s18n",
  "engines": {
    "node": ">=0.10"
  },
  "dependencies": {
    "async": "^2.0.0-rc.3",
    "commander": "^2.8.1",
    "globby": "^4.0.0",
    "htmlparser2": "^3.8.3",
    "mkdirp": "^0.5.1",
    "pify": "^2.2.0",
    "pinkie-promise": "^2.0.0",
    "update-notifier": "^0.7.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.4",
    "cz-conventional-changelog": "^1.1.5",
    "del": "^2.0.2",
    "ghooks": "^1.2.1",
    "gulp": "^3.9.0",
    "gulp-cached": "^1.1.0",
    "gulp-if": "^2.0.0",
    "gulp-istanbul-enforcer": "^1.0.3",
    "gulp-jscs": "^3.0.1",
    "gulp-jshint": "^2.0.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-mocha": "^2.1.3",
    "gulp-shell": "^0.5.0",
    "istanbul": "^0.4.2",
    "jshint": "^2.9.1",
    "jshint-stylish": "^2.0.1",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": "1.2.0",
    "pre-commit": "^1.0.10",
    "run-sequence": "^1.1.4",
    "sinon": "^1.17.1",
    "validate-commit-msg": "^2.6.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "helpMessage": "\nThis project uses commitizen to document changes. Please try:\nnpm install commitizen -g && git cz\n"
    }
  },
  "pre-commit": "test"
}
